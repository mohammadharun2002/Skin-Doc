    "import requests\n",
    "\n",
    "url = \"http://127.0.0.1:5001/predict\"\n",
    "file_path = \"Test Images/03.jpeg\"\n",
    "\n",
    "\n",
    "\n",
    "with open(file_path, \"rb\") as f:\n",
    "    files = {\"file\": f}\n",
    "    response = requests.post(url, files=files)\n",
    "    print(response.json())"
